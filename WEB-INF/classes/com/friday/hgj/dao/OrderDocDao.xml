<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<mapper namespace="com.friday.hgj.dao.OrderDocDao">
	
	<insert id="insert" parameterType="OrderDoc" useGeneratedKeys="true" keyProperty="id">
		insert into order_doc(orderId,docPath,remark,operateTime,operateId)
		values(#{orderId},#{docPath},#{remark},#{operateTime},#{operateId})
	</insert>
	
</mapper>
